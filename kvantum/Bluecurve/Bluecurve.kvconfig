frame.element=button
frame.top=2
frame.bottom=2
frame.left=2
frame.right=2
text.margin=true
text.margin.top=5
text.margin.bottom=4
text.margin.left=4
text.margin.right=4
indicator.size=10
indicator.element=arrow

[PanelButtonTool]
inherits=PanelButtonCommand

[IndicatorSpinBox]
inherits=PanelButtonCommand
indicator.element=spinbutton

