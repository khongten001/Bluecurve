slider_handle_length=24

small_icon_size=16
large_icon_size=48
button_icon_size=20
toolbar_icon_size=24

[Hacks]
normal_default_pushbutton=true

[GeneralColors]
window.color=#e6e6e6
base.color=#ffffff
alt.base.color=#eeeeee
button.color=#e6e6e6
light.color=#ffffff
mid.light.color=#f5f5f5
dark.color=#999999
mid.color=#c1c1c1
highlight.color=#4464ac
inactive.highlight.color=#5e7ab7
text.color=black
window.text.color=black
button.text.color=black
disabled.text.color=#777777
tooltip.text.color=black
